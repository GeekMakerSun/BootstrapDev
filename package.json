{
  "name": "bootstrapdev",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules assets/scss/:assets/css/",
    "css-lint": "stylelint assets/scss/",
    "css-prefix": "postcss --replace assets/css/main.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css assets/css/main.css --content index.html \"node_modules/bootstrap/js/dist/dom/*.js\" \"node_modules/bootstrap/js/dist/{base-component,dropdown,modal,offcanvas}.js\" --output assets/css/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm-run-all css-lint css"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.3",
    "sass": "^1.35.1",
    "serve": "^12.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.1"
  }
}
