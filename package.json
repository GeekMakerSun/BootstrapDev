{
  "name": "bootstrapdev",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace assets/css/main.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css assets/css/main.css --content index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output assets/css/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.2",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^12.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0"
  }
}
